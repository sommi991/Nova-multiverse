{
  "name": "nova-multiverse",
  "version": "1.0.0",
  "description": "The Ultimate Multi-Niche React Template - 6 Complete SPAs in One",
  "author": "sommi991",
  "license": "MIT",
  "private": false,
  "homepage": "https://novamultiverse.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/sommi991/nova-multiverse"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "admin-dashboard",
    "ecommerce",
    "crm",
    "multipurpose",
    "template",
    "multiniche"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "vite build --mode analyze",
    "deploy": "npm run build && npm publish"
  },
